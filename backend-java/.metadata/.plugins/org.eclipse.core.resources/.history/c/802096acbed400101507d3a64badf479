package com.project.beef.domain;

import lombok.*;
import jakarta.persistence.*; // ⭐ 주석 제거 및 import 활성화

@Entity // ⭐ 이 클래스는 DB 테이블과 매핑됩니다.
@Builder
@Getter
@Setter
@NoArgsConstructor
@AllArgsConstructor
public class Cut {
    
    @Id // ⭐ 기본 키 지정
    @GeneratedValue(strategy = GenerationType.IDENTITY) // ⭐ ID 자동 생성 설정
    private Long id; // ID 필드 활성화

    private String detectedPart;
    private String detectedGrade;
    private Integer marblingRatio;
    private String insight;
    private String fileName;
    // ... 나머지 DB 필드
}